		"gulp-plumber": "^1.2.1",
		"gulp-rename": "^1.4.0",
		"gulp-replace": "^1.0.0",
		"gulp-sass": "^4.0.2",
		"gulp-systemjs-builder": "^0.15.0",
		"gulp-tslint": "^8.1.4",
		"gulp-uglify": "^3.0.2",
		"hammerjs": "^2.0.8",
