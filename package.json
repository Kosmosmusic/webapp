	"scripts": {
		"prestart": "npm install && npm run clear-ts-build",
		"clear-ts-build": "find ./public/app/ ./test/client/ ./public/js/ -type f -name \"*.js\" -exec rm {} \\; && npm run clear-js-maps",
		"clear-js-maps": "find ./public/app/ ./test/client/ -type f -regex \".*\\.js\\..*\" -exec rm {} \\;",
		"start": "gulp",
		"pretest": "npm install",
		"test": "gulp test",
		"server-test": "gulp server-test",
		"client-test": "npm run clear-reports && gulp compile-and-test && npm run client-coverage-report",
		"client-coverage-report": "remap-istanbul -i ./logs/coverage/coverage-final.json -o ./logs/coverage/html-report -t html && echo '# coverage html-report generated, path: ./logs/coverage/html-report'",
		"clear-reports": "rm -rf ./logs/coverage ./logs/unit ./log && echo '# coverage and unit reports cleared'",
		"lint": "gulp lint",
		"preupdate-webdriver": "npm install",
		"update-webdriver": "webdriver-manager update",
		"preprotractor": "npm run update-webdriver",
		"protractor": "protractor test/protractor.conf.js",
		"set-env": "bash ./set-env.sh",
		"tsc": "tsc",
		"tsc:w": "tsc -w"
	}
