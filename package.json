{
  "name": "kosmosmusic",
  "version": "1.0.0",
  "private": true,
  "description": "Kosmosmusic - drum&bass label multimedia concentrator",
  "keywords": [
    "Angular",
    "ExpressJS",
    "JavaScript",
    "NodeJS",
    "TypeScript",
    "application"
  ],
  "homepage": "https://kosmosmusic-51fd1.firebaseapp.com",
  "bugs": "https://github.com/rfprod/kosmosmusic/issues",
  "repository": "https://github.com/rfprod/kosmosmusic",
  "license": "MIT",
  "author": "rfprod <connect@rfprod.tk> (https://github.com/rfprod)",
  "main": "server.js",
  "scripts": {
    "build": "bash shell/build.sh",
    "build-prod": "bash shell/build.sh prod",
    "build-prod-firebase": "bash shell/build.sh prod firebase",
    "e2e": "ng e2e",
    "express-prod": "node server.js prod",
    "postinstall": "npx sort-package-json",
    "install-firebase": "bash shell/install.sh firebase",
    "install-global": "bash shell/install.sh global",
    "install-local": "bash shell/install.sh",
    "install-project": "bash shell/install.sh project",
    "lint": "ng lint",
    "ng": "ng",
    "ng-build": "ng build",
    "ng-build-prod": "ng build --prod",
    "ng-build-prod-firebase": "ng build --prod --baseHref /kosmosmusic/",
    "pre-e2e": "npm run update-webdriver",
    "reset-client-app-env": "npx ts-node -O '{ \"module\": \"commonjs\", \"typeRoots\": [\"node_modules/@types\"] }' src/set-env.ts --reset=true",
    "set-client-app-env": "npx ts-node -O '{ \"module\": \"commonjs\", \"typeRoots\": [\"node_modules/@types\"] }' src/set-env.ts",
    "set-env": "bash shell/set-env.sh",
    "prestart": "npm install && npm run set-client-app-env",
    "start": "node server.js & ng serve",
    "test": "ng test",
    "test-single-run": "ng test kosmosmusic --watch=false",
    "test-single-run-and-report": "ng test kosmosmusic --watch=false --code-coverage",
    "test-single-run-and-report-to-dist": "ng test kosmosmusic --watch=false --code-coverage && cp -r ./coverage ./dist/coverage"
  },
